    <name>Group</name>
    <macros>
      <D>Ug</D>
    </macros>
    <x>380</x>
    <y>50</y>
    <width>220</width>
    <height>20</height>
    <style>3</style>
    <transparent>true</transparent>
    <widget type="label" version="2.0.0">
      <name>Label_2</name>
      <text>$(D)</text>
    </widget>
    <widget type="textupdate" version="2.0.0">
      <name>Text Update_1</name>
      <pv_name>$(P)$(D)_SP:sRdV</pv_name>
