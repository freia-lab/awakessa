    field(DTYP, "asynUInt32Digital")
    field(OUT, "@asynMask($(STUP_OUT) 0 0x0100)")
    field(ZNAM, "Remote Off")
    field(ONAM, "Remote On")
    field(ZSV, "MINOR")
    field(PINI, "YES")
}

