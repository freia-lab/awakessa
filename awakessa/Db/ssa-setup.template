    field(DTYP, "asynUInt32Digital")
    field(INP, "@asynMask($(STUP_IN) 0 0x0100)")
    field(ZNAM, "Local")
    field(ONAM, "Remote")
    field(ZSV, "MINOR")
    field(SCAN,"I/O Intr")
}

